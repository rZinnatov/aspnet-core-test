@{
    Layout = "_Layout";
}

@section Scripts
{
    <script src='~/lib/swagger-js/browser/swagger-client.min.js' type='text/javascript'></script>
    <script type="text/javascript">
        window.swagger = new SwaggerClient({
            url: "http://localhost:2244/swagger/v1/swagger.json",
            success: function() {
                swagger.Users.UsersGet(
                    { petId: 1 },
                    { responseContentType: 'application/json' },
                    function (data) {
                        console.log(data);
                    }
                );
            }
        });
    </script>
}
